{
  "name": "insights-inventory-frontend",
  "version": "0.0.1",
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@patternfly/react-core": "^3.124.1",
    "@patternfly/react-icons": "3.14.24",
    "@patternfly/react-table": "^2.24.64",
    "@redhat-cloud-services/frontend-components": "0.0.47",
    "@redhat-cloud-services/frontend-components-inventory-compliance": "0.0.34",
    "@redhat-cloud-services/frontend-components-inventory-general-info": "0.0.15",
    "@redhat-cloud-services/frontend-components-inventory-insights": "0.4.2",
    "@redhat-cloud-services/frontend-components-inventory-vulnerabilities": "1.24.4",
    "@redhat-cloud-services/frontend-components-notifications": "0.0.7",
    "@redhat-cloud-services/frontend-components-utilities": "0.0.15",
    "@redhat-cloud-services/host-inventory-client": "1.0.19",
    "classnames": "^2.2.6",
    "graphql": "^14.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.1.1",
    "react-router-dom": "4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/entry.js",
      "!src/entry-dev.js"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@redhat-cloud-services/frontend-components-config": "0.0.14",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.11.1",
    "fetch-mock": "^7.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.4.0",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "test": "jest",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelint.json",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "webpack-dev-server --config config/dev.webpack.config.js",
    "travis:build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run travis:verify"
  },
  "insights": {
    "appname": "inventory"
  }
}

{
  "name": "insights-inventory-frontend",
  "version": "0.0.1",
  "private": false,
  "dependencies": {
    "@patternfly/react-core": "4.90.2",
    "@patternfly/react-icons": "^4.8.4",
    "@patternfly/react-table": "4.20.15",
    "@redhat-cloud-services/frontend-components": "3.0.4",
    "@redhat-cloud-services/frontend-components-inventory-compliance": "^2.2.25",
    "@redhat-cloud-services/frontend-components-inventory-general-info": "3.0.2",
    "@redhat-cloud-services/frontend-components-inventory-insights": "^3.0.2",
    "@redhat-cloud-services/frontend-components-inventory-patchman": "^1.11.1",
    "@redhat-cloud-services/frontend-components-inventory-vulnerabilities": "^1.62.1",
    "@redhat-cloud-services/frontend-components-notifications": "3.0.3",
    "@redhat-cloud-services/frontend-components-remediations": "^3.0.0",
    "@redhat-cloud-services/frontend-components-utilities": "3.0.2",
    "@redhat-cloud-services/host-inventory-client": "1.0.93",
    "classnames": "^2.2.6",
    "graphql": "^15.5.0",
    "marked": "^1.2.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.12.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/entry.js",
      "!src/entry-dev.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@patternfly/react-core/|@patternfly/react-icons/|@redhat-cloud-services|@patternfly/react-table|@patternfly/react-tokens)).*$"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "customReact": "react",
      "PFReactCore": "@patternfly/react-core",
      "PFReactTable": "@patternfly/react-table",
      "reactRedux": "react-redux"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@redhat-cloud-services/frontend-components-config": "4.0.1",
    "@redhat-cloud-services/frontend-components-inventory": "2.4.45",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.22.0",
    "fetch-mock": "^9.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^13.9.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "webpack": "^5.20.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "whatwg-fetch": "^3.5.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "test": "jest",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelint.json",
    "prod": "NODE_ENV=production webpack serve --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "webpack serve --config config/dev.webpack.config.js",
    "travis:build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "travis:verify": "npm-run-all travis:build lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run travis:verify"
  },
  "insights": {
    "appname": "inventory"
  }
}
